{
  "name": "api-generate-module",
  "version": "1.0.0",
  "description": "library to produce restfull api based on database schema using Prisma ORM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "generate-api": "ts-node src/generators/generatorRestApi.ts",
    "update-tsconfig": "ts-node src/generators/updateTsconfig.ts",
    "generate-and-update": "npm run generate-api && npm run update-tsconfig"
  },
  "keywords": [],
  "author": "Handika",
  "license": "ISC",
  "dependencies": {
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "prisma": "^5.17.0",
    "uninstall": "^0.0.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@prisma/client": "^5.17.0",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.12",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
